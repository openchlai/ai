{
  "uid": "ai-service-system-overview",
  "title": "AI Service - System Overview",
  "tags": ["ai-service", "live", "overview"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "5s",
  "editable": true,
  "graphTooltip": 1,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Requests/sec",
      "type": "stat",
      "gridPos": {"h": 5, "w": 6, "x": 0, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_requests_total[1m]))",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 1,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 50, "color": "red"}
            ]
          },
          "mappings": []
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "justifyMode": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 2,
      "title": "Active Tasks",
      "type": "stat",
      "gridPos": {"h": 5, "w": 6, "x": 6, "y": 0},
      "targets": [{
        "expr": "sum(celery_worker_tasks_active)",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "blue"},
              {"value": 5, "color": "orange"},
              {"value": 20, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
      }
    },
    {
      "id": 3,
      "title": "GPU Utilization",
      "type": "gauge",
      "gridPos": {"h": 5, "w": 6, "x": 12, "y": 0},
      "targets": [{
        "expr": "DCGM_FI_DEV_GPU_UTIL",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 50, "color": "yellow"},
              {"value": 80, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 4,
      "title": "Avg Response Time",
      "type": "stat",
      "gridPos": {"h": 5, "w": 6, "x": 18, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_request_duration_seconds_sum[1m])) / sum(rate(http_request_duration_seconds_count[1m])) * 1000",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 100, "color": "yellow"},
              {"value": 500, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
      }
    },
    {
      "id": 5,
      "title": "Request Rate (Live)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 5},
      "targets": [{
        "expr": "sum(rate(http_requests_total[1m]))",
        "legendFormat": "Requests/sec",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 3,
            "fillOpacity": 40,
            "gradientMode": "opacity",
            "showPoints": "never",
            "spanNulls": true
          }
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      }
    },
    {
      "id": 6,
      "title": "Response Time Distribution",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 5},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[1m])) by (le)) * 1000",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (le)) * 1000",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[1m])) by (le)) * 1000",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "hue",
            "showPoints": "never",
            "spanNulls": true
          }
        }
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "last"]},
        "tooltip": {"mode": "multi"}
      }
    },
    {
      "id": 7,
      "title": "System Resources",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 13},
      "targets": [
        {
          "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[1m])) * 100)",
          "legendFormat": "CPU %",
          "refId": "A"
        },
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
          "legendFormat": "Memory %",
          "refId": "B"
        },
        {
          "expr": "DCGM_FI_DEV_GPU_UTIL",
          "legendFormat": "GPU %",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "opacity",
            "showPoints": "never",
            "spanNulls": true
          }
        }
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "last"]},
        "tooltip": {"mode": "multi"}
      }
    },
    {
      "id": 8,
      "title": "GPU Temperature",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 13},
      "targets": [{
        "expr": "DCGM_FI_DEV_GPU_TEMP",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "min": 0,
          "max": 100,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 70, "color": "yellow"},
              {"value": 80, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 9,
      "title": "Model Task Success Rate",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 13},
      "targets": [
        {
          "expr": "sum(celery_task_succeeded_total)",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(celery_task_failed_total)",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ],
      "options": {
        "pieType": "donut",
        "displayLabels": ["percent"],
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 10,
      "title": "Top Endpoints by Traffic",
      "type": "bargauge",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 21},
      "targets": [{
        "expr": "topk(10, sum(rate(http_requests_total[5m])) by (endpoint))",
        "legendFormat": "{{endpoint}}",
        "refId": "A"
      }],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {"mode": "continuous-GrYlRd"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 11,
      "title": "Celery Queue Length",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 21},
      "targets": [{
        "expr": "sum(celery_queue_length) by (queue_name)",
        "legendFormat": "{{queue_name}}",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 0,
            "fillOpacity": 80,
            "gradientMode": "hue",
            "showPoints": "never",
            "spanNulls": false
          }
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      }
    }
  ]
}
