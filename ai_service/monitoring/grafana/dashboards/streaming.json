{
  "uid": "ai-service-streaming",
  "title": "AI Service - Real-time Streaming",
  "tags": [
    "ai-service",
    "streaming",
    "websocket"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Active Streaming Sessions",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(streaming_active_sessions)",
          "legendFormat": "Total Sessions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "gray"
              },
              {
                "value": 1,
                "color": "green"
              },
              {
                "value": 10,
                "color": "yellow"
              },
              {
                "value": 50,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "WebSocket Sessions",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "streaming_active_sessions{type=\"websocket\"}",
          "legendFormat": "WebSocket"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "gray"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "TCP Sessions",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "expr": "streaming_active_sessions{type=\"tcp\"}",
          "legendFormat": "TCP"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "gray"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Active Sessions Over Time",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "streaming_active_sessions",
          "legendFormat": "{{type}}"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Sessions"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 5,
      "title": "Streaming Latency (p50, p95, p99)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(streaming_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(streaming_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(streaming_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ],
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [
                2
              ],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "params": [],
              "type": "avg"
            },
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
        "for": "5m",
        "frequency": "1m",
        "handler": 1,
        "message": "Streaming latency is too high",
        "name": "High Streaming Latency",
        "noDataState": "no_data"
      }
    },
    {
      "id": 6,
      "title": "Average Streaming Latency",
      "type": "gauge",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(streaming_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "Latency"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "max": 5,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 2,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 7,
      "title": "Audio Buffer Size",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "streaming_buffer_size_bytes",
          "legendFormat": "Session {{session_id}}"
        }
      ],
      "yaxes": [
        {
          "format": "bytes",
          "label": "Buffer Size"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 8,
      "title": "Latency by Session Type",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(streaming_latency_seconds_bucket[5m])) by (le, session_type))",
          "legendFormat": "{{session_type}} p95"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 9,
      "title": "WebSocket Connections",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "expr": "streaming_active_sessions{type=\"websocket\"}",
          "legendFormat": "Active WebSocket"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Connections"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 10,
      "title": "TCP Connections",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 28
      },
      "targets": [
        {
          "expr": "streaming_active_sessions{type=\"tcp\"}",
          "legendFormat": "Active TCP"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Connections"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 11,
      "title": "Session Duration Distribution",
      "type": "heatmap",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 36
      },
      "targets": [
        {
          "expr": "sum(rate(streaming_latency_seconds_bucket[5m])) by (le)",
          "format": "heatmap",
          "legendFormat": "{{le}}"
        }
      ],
      "options": {
        "calculate": true,
        "calculation": {},
        "cellGap": 2,
        "color": {
          "scheme": "Turbo"
        }
      }
    },
    {
      "id": 12,
      "title": "Streaming Errors",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 44
      },
      "targets": [
        {
          "expr": "sum(rate(api_requests_total{endpoint=~\".*streaming.*\", status=~\"5..\"}[5m])) by (endpoint, status)",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "endpoint": "Endpoint",
              "status": "Status Code",
              "Value": "Error Rate"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 0.01,
                "color": "yellow"
              },
              {
                "value": 0.1,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 13,
      "title": "Buffer Fill Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 52
      },
      "targets": [
        {
          "expr": "rate(streaming_buffer_size_bytes[1m])",
          "legendFormat": "Session {{session_id}}"
        }
      ],
      "yaxes": [
        {
          "format": "Bps",
          "label": "Fill Rate"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 14,
      "title": "Session Health Status",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 52
      },
      "targets": [
        {
          "expr": "count(streaming_active_sessions > 0)",
          "legendFormat": "Healthy Sessions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [
            {
              "value": "null",
              "text": "No Active Sessions",
              "color": "gray"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "gray"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    }
  ]
}